public class NewMembershipsPayment {

    /*
		returnSalesWrapper_a ()
		--
		Generate the HTML form for the payment gateway
    */
    @AuraEnabled
    public static VerifoneCalloutUtility returnSalesWrapper_a (Id recordId) {
        return NewMembershipsPaymentHelper.buildSalesWrapper (recordId);
    }

    /*
        autoConvertDirectDebit_a ()
        --
        Convert the Lead once payment has been taken
    */
    @AuraEnabled
    public static String autoConvertDirectDebit_a (Id recordId, String accountName, String accountNumber, String sortCode) {
        System.debug('[autoConvertDirectDebit_a] paymentMethod: '+accountNumber);
        return NewMembershipsPaymentHelper.convertLead (recordId, accountName, accountNumber, sortCode);
    }
    
    /*
        autoConvertCashCheque_a ()
        --
        Convert the Lead once payment has been taken
    */
    @AuraEnabled
    public static String autoConvertCashCheque_a (Id recordId, String paymentMethod) {
        System.debug('[autoConvertCashCheque_a] paymentMethod: '+paymentMethod);
        return NewMembershipsPaymentHelper.convertLead (recordId, paymentMethod);
    }

    /*
        updateLead_a ()
        --
        Update lead details
    */
    @AuraEnabled
    public static String updateLead_a (Id recordId, String paymentMethod) {
        System.debug('[updateLead_a] paymentMethod: '+paymentMethod);
        return NewMembershipsPaymentHelper.updateLeadDetails (recordId, paymentMethod);
    }
    
    


